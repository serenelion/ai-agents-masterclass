# PostgreSQL Configuration
POSTGRES_USER=jaguar_user
POSTGRES_PASSWORD=your_secure_password_here_change_this
POSTGRES_DB=n8n

# N8N Configuration
N8N_ENCRYPTION_KEY=your_secure_encryption_key_here_change_this_32_chars_min
N8N_USER_MANAGEMENT_JWT_SECRET=your_secure_jwt_secret_here_change_this_32_chars_min

# Production URLs
PRODUCTION_N8N_URL=https://n8n.thespatialnetwork.net
PRODUCTION_OPENWEBUI_URL=https://ai.thespatialnetwork.net

# Qdrant Configuration
QDRANT_URL=http://jaguar-qdrant:6333
QDRANT_API_KEY=your_qdrant_api_key_here_optional

# GitHub Configuration (optional)
GITHUB_TOKEN=your_github_token_here_optional
GITHUB_ORG=The-Spatial-Network

# OpenAI Configuration (for embeddings and fallback)
OPENAI_API_KEY=your_openai_api_key_here_optional

# Security Settings
ENABLE_SIGNUP=false
DEFAULT_USER_ROLE=user
ENABLE_COMMUNITY_SHARING=false
